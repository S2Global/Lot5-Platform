<div class="container">
	<h2>Sessions</h2>
	<table class="table table-light shadow">
		<thead>
			<tr>
				<th>World</th>
				<th>ID</th>
				<th>Notes</th>
				<th colspan="300"></th>
			</tr>
		</thead>
		<tbody>
			<% @sessions.each do |session| %>
				<tr>
					<td><%= link_to session.world.name, '/worlds/' + session.world_id.to_s + '/sessions/' + session.id.to_s %></td>
					<td><%= session.id %></td>
					<td><%= session.notes %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
</div>